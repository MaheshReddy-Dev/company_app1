<%= form_with(model: @project, local: true) do |f| %>

  <p>
    <%= f.label :Title %><br/>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :Description %><br/>
    <%= f.text_area :description %>
  </p>
    <%= f.hidden_field :employee_id, value: params[:employee_id]%>
  <p>
    <%= f.submit %>
  </p>
<% end %>
